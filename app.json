{
  "expo": {
    "name": "earth-explore-app",
    "slug": "earth-explore-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Earth-Explore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Earth-Explore.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "68c82a0b-b3a9-4585-9e62-1236ac7d9c92"
      }
    },
    "owner": "earth-explore",
    "android": {
      "package": "com.zhangfulin.earthexploreapp",
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      }
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000",
      "visible": "translucent"
    }
  }
}
