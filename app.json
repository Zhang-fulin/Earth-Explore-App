{
  "expo": {
    "name": "earth-explore-app",
    "slug": "earth-explore-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Earth-Explore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "136a043d-06b4-4dc6-81c1-a2d41173d469"
      }
    },
    "owner": "zhangfulin",
    "android": {
      "package": "com.zhangfulin.earthexploreapp",
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      }
    }
  }
}
